#!/bin/bash

set -o errexit
set -o nounset

source macos.sh

function main() {
    local sysname="$(uname -s)"
    case "${sysname}" in
        Darwin*)
            bootstrap_macos
            ;;
        *)
            printf "system:[%s] is not supported right now\n" "${sysname}"
            ;;
    esac
}

main
